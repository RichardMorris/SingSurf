<definition name="Dupin Cyclides" type="asurf">
(r1^2 - dy^2 - (dx + r0)^2) (r1^2 - dy^2 - (dx - r0)^2)*
(x^4+y^4+z^4)+ 2((r1^2 - dy^2 - (dx + r0)^2 )*
(r1^2 - dy^2 - (dx - r0)^2) (x^2 y^2+x^2 z^2+y^2 z^2))+
2 ri^2 ((-dy^2-dx^2+r1^2+r0^2) (2 x dx+2 y dy-ri^2)-4 dy r0^2 y)*
(x^2+y^2+z^2)+ 4 ri^4(dx x+dy y) (-ri^2+dy y+dx x)+
4 ri^4 r0^2 y^2+ri^8-0;
<variable name="x" min="-2.01" max="2.22">
<variable name="y" min="-2.03" max="2.04">
<variable name="z" min="-2.05" max="2.16">
<parameter name="r0" value="2.1" />
<parameter name="r1" value="2" />
<parameter name="dx" value="2" />
<parameter name="dy" value="0" />
<parameter name="ri" value="2" />
</definition>

<definition name="Boy's Surface" type="asurf">
64 (1-z)^3 z^3- 48 (1-z)^2 z^2 (3 x^2+3 y^2+2 z^2)+
12 (1-z) z (27 (x^2+y^2)^2-24 z^2 (x^2+y^2)+
36 sqrt(2) y z (y^2-3 x^2)+4 z^4)+
(9 x^2+9 y^2-2 z^2) (-81 (x^2+y^2)^2-72 z^2 (x^2+y^2)+
108 sqrt(2) x z (x^2-3 y^2)+4 z^4)-0; 
<variable name="x" min="-1.14" max="2.03">
<variable name="y" min="-2.13" max="2.04">
<variable name="z" min="-1.12" max="2.05">
<option name="coarse" value="16">
<option name="fine" value="64">
<option name="face" value="2048">
</definition>

<definition name="Diagonal Surface of Clebsch" type="asurf">
//# This surface contains 27 straight lines

16 x^3 + 16 y^3 - 31 z^3 + 24 x^2 z - 48 x^2 y - 48 x y^2
+24 y^2 z - 54 sqrt(3) z^2 - 72 z;
<variable name="x" min="-4.14" max="4.03">
<variable name="y" min="-4.13" max="4.04">
<variable name="z" min="-4.12" max="4.05">
</definition>

<definition name="Bohemian Dome" type="asurf">
//# The envelope of a set of circles
//# in planes parallel to the x-z plane whose 
//# centres lie on a circle in the y-z plane.
//# Thanks to John Chertok and Giorgio Ferrarese for this.
// Degree 4

y^4 -( A^2 + B^2 + 6 A B + 2 ab ( A + B));
ab = y^2 - A - B;
A = 1-x^2; B = 1-z^2;
<variable name="x" min="-2.14" max="2.03">
<variable name="y" min="-2.13" max="2.04">
<variable name="z" min="-2.12" max="2.05">
</definition>

<definition name="Cayley's Cubic" type="asurf">
//# A cubic surface with 4 nodes
//# all other cubics with 4 nodes are projectivly equivilent to this one
4 (x^2+y^2+z^2) + 16 x y z - 1;
<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
</definition>

<definition name="Tetrahedral Surface" type="asurf">
//# Degree 4, 4 nodes, Tetrahedral symmetry
(x^2 + y^2 + z^2)^2 + 8*x*y*z - 10*(x^2 + y^2 + z^2) + 25-0;
<variable name="x" min="-3.14" max="3.03">
<variable name="y" min="-3.13" max="3.04">
<variable name="z" min="-3.12" max="3.05">

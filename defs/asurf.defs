<option name="fine" value="64">
<option name="face" value="2048">
</definition>

<definition name="Dervish" type="asurf">
//# A Quintic Surface with 31 nodes
//# Frow MathWorld/Enderass/Barth/Nordstrand
// http://mathworld.wolfram.com/Dervish.html
a F + q;
F = h1 h2 h3 h4 h5;
h1 = x - z;
h2 = c2pi5 x - s2pi5 y - z;
h3 = c4pi5 x - s4pi5 y - z;
h4 = c6pi5 x - s6pi5 y - z;
h5 = c8pi5 x - s8pi5 y - z;
c2pi5 = 0.30901699437494745; // cos(2 pi/5)
s2pi5 = 0.9510565162951535;
c4pi5 = -0.8090169943749473;
s4pi5 = 0.5877852522924732;
c6pi5 = -0.8090169943749475;
s6pi5 = -0.587785252292473;
c8pi5 = 0.30901699437494723;
s8pi5 = -0.9510565162951536;

q = (1-c z)(x^2 + y^2 - 1 + r z^2)^2;

r = (1+3 sqrt(5))/4;
a = - (8 / 5) (1 + 1/sqrt(5)) sqrt(5-sqrt(5));
c = sqrt(5-sqrt(5)) / 2;
<variable name="x" min="-6.24" max="6.13" />
<variable name="y" min="-6.23" max="6.14" />
<variable name="z" min="-6.22" max="6.15" />
<parameter name="w" value="2" />
<option name="coarse" value="16">
<option name="fine" value="64">
<option name="face" value="2048">
</definition>


<definition name="Sarti's Sextic" type="asurf">
//# A sextic with 48 nodes
S6+lambda*Q6-0;
Q6=(4*z^2+4+4*x^2+4*y^2)^3;
//# simplified version of Q6 =(xx^2+yy^2+zz^2+ww^2)^3;
S6=xx^6+yy^6+zz^6+ww^6+15.0*(xx^2*yy^2*zz^2+xx^2*yy^2*ww^2+xx^2*zz^2*ww^2+yy^2*zz^2*ww^2);
xx=z-1+sqrt(2)*x;
yy=z-1-sqrt(2)*x;
zz=-(z+1+sqrt(2)*y);
ww=-(z+1-sqrt(2)*y);
w=1;lambda=-7.0/12.0;
<variable name="x" min="-4.14" max="4.03">
<variable name="y" min="-4.13" max="4.04">
<variable name="z" min="-4.12" max="4.05">
<option name="coarse" value="16">
<option name="fine" value="64">
<option name="face" value="2048">
</definition>

<definition name="Barth's Sextic" type="asurf">
//# A Sextic with 65 nodes
4 ( t^2 x^2 - y^2 ) ( t^2 y^2 - z^2 ) ( t^2 z^2 - x^2 )
- ( 1 + 2 t) (x^2 + y^2 + z^2 - 1)^2 - 0;
t = 1.618034; 
<variable name="x" min="-2.14" max="2.03">
<variable name="y" min="-2.13" max="2.04">
<variable name="z" min="-2.12" max="2.05">
</definition>

<definition name="St�bchen" type="asurf">

<definition name="Togliatti surfaces" type="asurf">
// http://enriques.mathematik.uni-mainz.de/docs/Etogliatti.shtml
// A quintics with 31 nodes
64 (x-w) (x^4-4 x^3 w - 10 x^2 y^2 - 4 x^2 w^2 
            + 16 x w^3 - 20 x y^2 w + 5 y^4 + 16 w^4 - 20 y^2 w^2)
  - 5 sqrt(5 - sqrt(5)) (2 z - sqrt(5 - sqrt(5)) w)
     ( 4 (x^2+y^2 - z^2) + (1+3 sqrt(5))w^2)^2;

<variable name="x" min="-14.24" max="14.03" />
<variable name="y" min="-14.23" max="14.04" />
<variable name="z" min="-14.22" max="14.05" />
<parameter name="w" value="2" />

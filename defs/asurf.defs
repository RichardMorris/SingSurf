<variable name="x" min="-1.14" max="1.03">
<variable name="y" min="-1.13" max="1.04">
<variable name="z" min="-1.12" max="1.05">
<option name="coarse" value="16">
<option name="fine" value="16">
<option name="face" value="2048">
</definition>

<definition name="Kummer Surface" type="asurf">
//# A family of quartic surfaces 
//# some of which have 16 nodes
//# Change the value of v to get different surface
//# try v=1; v=1.1; v=1.5; v=1.7; v=1.8; v=2;

(3-v^2) ( x^2+y^2 + z^2 - v^2)^2 - (3 v^2 - 1) p q r s - 0;
p = 1 - z - x rt2; q = 1 - z + x rt2;
r = 1 + z + y rt2; s = 1 + z - y rt2;
rt2 = sqrt(2);
<variable name="x" min="-1.54" max="1.63">
<variable name="y" min="-1.53" max="1.64">
<variable name="z" min="-1.52" max="1.65">
<parameter name="v" value="1.2" />
<option name="coarse" value="16">
<option name="fine" value="64">
<option name="face" value="2048">
</definition>

